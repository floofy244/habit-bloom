{
  "version": 2,
  "builds": [
    {
      "src": "habitbloom/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "habitbloom/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "habitbloom/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "habitbloom/wsgi.py"
    },
    {
      "src": "/media/(.*)",
      "dest": "habitbloom/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "habitbloom.settings"
  }
}
